@model IEnumerable<CupcakeYPasteles.Models.DineroEnCaja>

@{
    ViewBag.Title = "Index";
}

<h1 class="text-center">Dinero en caja</h1>
<hr />

<table class="table table-striped table-bordered" id="tablacaja" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.fecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dinero)
            </th>
         
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.fecha)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.dinero)
                </td>
                
            </tr>
        }
    </tbody>

</table>
